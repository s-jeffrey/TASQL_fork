"(SELECT AVG(ym.Consumption) AS SME_Avg\n FROM yearmonth ym\n INNER JOIN customers c ON ym.CustomerID = c.CustomerID\n WHERE c.Currency = 'CZK'\n AND c.Segment = 'SME'\n AND SUBSTR(ym.Date, 1, 4) = '2013') AS SME_Avg,\n\n(SELECT AVG(ym.Consumption) AS LAM_Avg\n FROM yearmonth ym\n INNER JOIN customers c ON ym.CustomerID = c.CustomerID\n WHERE c.Currency = 'CZK'\n AND c.Segment = 'LAM'\n AND SUBSTR(ym.Date, 1, 4) = '2013') AS LAM_Avg,\n\n(SELECT AVG(ym.Consumption) AS KAM_Avg\n FROM yearmonth ym\n INNER JOIN customers c ON ym.CustomerID = c.CustomerID\n WHERE c.Currency = 'CZK'\n AND c.Segment = 'KAM'\n AND SUBSTR(ym.Date, 1, 4) = '2013') AS KAM_Avg\n\nFROM dual;\n\nSELECT SME_Avg - LAM_Avg AS SME_LAM_Diff,\n       LAM_Avg - KAM_Avg AS LAM_KAM_Diff,\n       KAM_Avg - SME_Avg AS KAM_SME_Diff\nFROM (SELECT\n      (SELECT AVG(ym.Consumption)\n       FROM yearmonth ym\n       INNER JOIN customers c ON ym.CustomerID = c.CustomerID\n       WHERE c.Currency = 'CZK'\n       AND c.Segment = 'SME'\n       AND SUBSTR(ym.Date, 1, 4) = '2013') AS SME_Avg,\n\n      (SELECT AVG(ym.Consumption)\n       FROM yearmonth ym\n       INNER JOIN customers c ON ym.CustomerID = c.CustomerID\n       WHERE c.Currency = 'CZK'\n       AND c.Segment = 'LAM'\n       AND SUBSTR(ym.Date, 1, 4) = '2013') AS LAM_Avg,\n\n      (SELECT AVG(ym.Consumption)\n       FROM yearmonth ym\n       INNER JOIN customers c ON ym.CustomerID = c.CustomerID\n       WHERE c.Currency = 'CZK'\n       AND c.Segment = 'KAM'\n       AND SUBSTR(ym.Date, 1, 4) = '2013') AS KAM_Avg) AS AvgConsumptions;"